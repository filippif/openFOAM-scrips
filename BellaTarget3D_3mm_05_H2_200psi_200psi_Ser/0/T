FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inlet_1
    {
        type            fixedValue;
        value           uniform 300;
    }
    inlet_2
    {
        type            fixedValue;
        value           uniform 300;
    }
    vacuum
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    capillary
    {
        type            zeroGradient;
    }
}


